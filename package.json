{
  "name": "frogs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development bankai start -p 8080",
    "build": "npm run build-icons && npm run build-app && npm run build-staticfix",
    "build-app": "NODE_ENV=production bankai build",
    "build-staticfix": "mkdir -p ./dist && rm -f ./dist/manifest.json && cp ./manifest.json ./dist/manifest.json && rm -f ./dist/favicon.ico && cp ./favicon.ico ./dist/favicon.ico && rm -f ./dist/sw.js && cp ./sw.js ./dist/sw.js && rm -f ./dist/_redirects && cp ./_redirects ./dist/_redirects",
    "build-icons": "svgo -f assets",
    "inspect": "bankai inspect",
    "test": "standard"
  },
  "author": "srilq <git@srilq.email>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babelify": "^7.3.0",
    "bankai": "^9.15.0",
    "brfs": "^2.0.1",
    "standard": "^11.0.1",
    "svgo": "^0.7.2"
  },
  "dependencies": {
    "choo": "^6.1.0",
    "choo-devtools": "^1.3.1",
    "choo-log": "^7.2.1",
    "choo-service-worker": "^2.3.0",
    "nanoanimation": "^1.0.0",
    "tachyons": "^4.8.1",
    "unique-random-array": "^1.0.0",
    "web-animations-js": "^2.3.1",
    "xhr": "^2.4.0"
  },
  "browser": {
    "request": "xhr"
  },
  "browserify": {
    "transform": [
      "brfs",
      "babelify"
    ]
  },
  "babel": {
    "plugins": [
      "transform-es2015-arrow-functions"
    ]
  }
}
